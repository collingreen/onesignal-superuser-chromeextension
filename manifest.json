{
  "name": "OneSignal SuperUser Quick Link",
  "description": "Quickly link to onesignal superuser pages for a given UUID or email",
  "version": "1.0",
  "manifest_version": 3,
  "action": {
    "default_popup": "onesignalsuperuser.html",
    "default_icon": "onesignalsuperuser.png"
  },
  "background": {
    "service_worker": "background.js"
  },
  "permissions": [
    "contextMenus"
  ],
  "icons": {
    "16": "onesignalsuperuser.png"
  }
}
